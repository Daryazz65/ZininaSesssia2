<Window x:Class="ZininaSesssia2.View.Windows.UserrWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZininaSesssia2.View.Windows"
        mc:Ignorable="d"
        Title="UserrWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" FontSize="25" HorizontalAlignment="Center"><Hyperlink Name="OneHl"
                    Click="OneHl_Click">Журнал</Hyperlink></TextBlock>
        <TextBlock Grid.Row="1" FontSize="25" HorizontalAlignment="Center"><Hyperlink Name="TwoHl" 
                    Click="TwoHl_Click">Мой профиль</Hyperlink></TextBlock>
    </Grid>
</Window>